{
  "name": "body-fat-calculator",
  "version": "1.0.0",
  "main": "App.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage --verbose",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-versions": "node scripts/check-versions.js",
    "nuke": "rm -rf node_modules/ && pnpm install && cd ios && rm -rf Pods Podfile.lock && pod install && cd .."
  },
  "dependencies": {
    "@expo-google-fonts/montserrat": "^0.2.3",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native-stack": "^7.1.10",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "expo": "^54.0.0",
    "expo-application": "~7.0.7",
    "expo-dev-client": "~6.0.12",
    "expo-device": "~8.0.7",
    "expo-file-system": "~19.0.14",
    "expo-font": "~14.0.8",
    "expo-localization": "~17.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "posthog-react-native": "^3.15.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-keyboard-controller": "^1.18.5",
    "react-native-purchases": "^8.9.6",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.13.0",
    "react-native-worklets": "^0.5.1",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/js": "^9.35.0",
    "@react-navigation/native": "^7.0.13",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/react-native": "^12.8.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.13",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "chalk": "^4.1.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.11",
    "prettier": "^3.2.5",
    "typescript": "~5.9.2"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    },
    "install": {
      "exclude": [
        "react-native-svg"
      ]
    }
  },
  "engines": {
    "node": "22.11.0"
  }
}
