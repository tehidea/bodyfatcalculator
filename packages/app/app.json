{
  "expo": {
    "name": "Body Fat",
    "slug": "body-fat-calculator",
    "owner": "tehidea",
    "scheme": "bodyfatcalculator",
    "extra": {
      "eas": {
        "projectId": "9f81ba56-4efb-41b5-8cb7-a310221df724"
      }
    },
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tehidea.bodyfatcalculator",
      "buildNumber": "16",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tehidea.bodyfatcalculator",
      "versionCode": 16,
      "permissions": [
        "android.permission.health.READ_BODY_FAT",
        "android.permission.health.WRITE_BODY_FAT",
        "android.permission.health.WRITE_WEIGHT",
        "android.permission.health.WRITE_HEIGHT",
        "android.permission.health.WRITE_LEAN_BODY_MASS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "../../node_modules/@expo-google-fonts/montserrat/200ExtraLight/Montserrat_200ExtraLight.ttf",
            "../../node_modules/@expo-google-fonts/montserrat/300Light/Montserrat_300Light.ttf",
            "../../node_modules/@expo-google-fonts/montserrat/400Regular/Montserrat_400Regular.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "imageResizeMode": "contain"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Body Fat Calculator uses your photos to attach progress images to measurements.",
          "cameraPermission": "Body Fat Calculator uses the camera to take progress photos."
        }
      ],
      "expo-localization",
      [
        "expo-notifications",
        {
          "defaultChannel": "reminders"
        }
      ],
      [
        "react-native-cloud-storage",
        {
          "iCloudContainerEnvironment": "Production",
          "iCloudContainerIdentifier": "iCloud.com.tehidea.bodyfatcalculator"
        }
      ],
      [
        "@kingstinct/react-native-healthkit",
        {
          "NSHealthShareUsageDescription": "Body Fat Calculator reads your body fat data to display trends",
          "NSHealthUpdateUsageDescription": "Body Fat Calculator saves your body fat, weight, height, and body composition results"
        }
      ],
      "react-native-health-connect",
      "./plugins/withHealthConnectPermissions",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "bodyfatcalculator",
          "organization": "tehidea"
        }
      ]
    ]
  }
}
